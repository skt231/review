<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.base.dao.IBaseDao">
	<insert id="insert">
		insert into human(name,age,height,birthday)
		values(#{name},#{age},#{height},#{birthday})
	</insert>
	<select id="selectAll" resultType="com.base.dto.BaseDto">
		select * from human
	</select>
	<select id="selectName" resultType="com.base.dto.BaseDto">
		select * from human where
		name=#{name}
	</select>
	<update id="update">
		update human set height=#{height} where name=#{name}
	</update>
	<delete id="delete">
		delete human where name=#{name}
	</delete>
</mapper>